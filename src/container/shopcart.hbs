<div>
    <ul class="nav">
         <li class="nav-item">
            <a class="nav-link" href="'https://github.com/">Home</a>
         </li>
         <li class="nav-item">
            <a class="nav-link" href="https://www.google.com/">Contacts</a>
         </li>
         <li class="nav-item">
            <a class="nav-link disabled" aria-disabled="true">Help</a>
        </li>
    </ul>
    <hr/>
</div>
<div>
    {{#with header}}
    <h1 class="title mb-2">{{title}}</h1>
    {{#with button}}
    <a href="{{href}}" class="btn btn-outline-secondary mb-2" role="button" data-bs-toggle="button">{{text}}</a>
    {{/with}}
    {{/with}}
    <hr/>  
</div>
<div>
    {{#each goodsBlock}}
        <ul>
            <div class="card mb-3" style="max-width:560px;">
                <div class="row g-0">
                    <div class="col md-4">
                        <img src="{{image}}" class="img-fluid rounded-start" alt="...">
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                             <h5 class="card-title">{{name}}</h5>
                              <table class="table table-borderless">
                                <tr>
                                    <td><p class="card-text">{{description}}</p></td>
                             {{#with price}}
                                    <td><p class="card-text">{{title}}</p><b>{{value}}</b></td>
                             {{/with}}
                              <td><p>{{amount}}</p></td>
                              <td>
                                <a href="#" class="btn btn-primary mb-1 md-4">Додати</a>
                                <a href="#" class="btn btn-outline-danger md-4">Видалити</a>
                              </td>
                              </tr>
                              </table>
                        </div>
                    </div>
                </div>
            </div>
        </ul>
        {{/each}}
<hr/>
</div>
<div>
{{#with total}}
  <h5 class="title mb-3">{{title}}</h5>
  <h3 class="title mb-3">{{amount}}</h3>
  <p class="mb-3">{{delivery}}</p>
  <a href="https://www.youtube.com/" class="btn btn-success mb-3" role="button" data-bs-toggle="button">Оформити замовлення</a><br>
  <a href="https://www.youtube.com/" class="btn btn-outline-success" role="button" data-bs-toggle="button">Офрмити кредит</a>
{{/with}}
<hr/>
</div>
<div>
    {{#with goodsOtherBlock}}
        <h3 class="title text-center">{{title}}</h3>
         {{#each cards}}
            <div class="card-group">
                    <div class="card">
                    <img src="{{image}}" class="card-img-top" alt="...">
                    <div class="card-body">
                     <h5 class="card-title">{{title}}</h5>
                     <p class="card-text">{{description}}</p>
                     <a href="#" class="btn btn-warning mb-3" role="button" data-bs-toggle="button">HOT</a>
                     <a href="#" class="btn btn-primary mb-3" role="button" data-bs-toggle="button">NEW</a>
                    </div>
            </div>
        {{/each}}
    {{/with}}
</div>
<hr/>
<div>
 <nav class="navbar bg-dark border-bottom border-body" data-bs-theme="dark">
     {{#with subscribe}}
        <div class="container-fluid">
         <span class="navbar-text">
         <h3 class="title text-start">{{header}}</h3>
         <p>{{description}}</p>
         </span>
         <form class="container-fluid justify-content-start">
           <a href="#" class="btn btn-primary mb-3" role="button" data-bs-toggle="button">Register Now</a><br>
           <a href="#" class="btn btn-primary mb-3" role="button" data-bs-toggle="button">Buy Subscription</a>
         </form>
        </div>
     {{/with}}
 </nav>
 <hr/>
</div>
<div>
    {{#each footer }}
        <ul class="nav flex-column">
            <li class="nav-item">
                <a class="nav-link" href="{{link}}">{{text}}</a>
            </li>
        </ul>
    {{/each}}
</div>